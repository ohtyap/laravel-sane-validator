{
    "name": "ohtyap/laravel-sane-validator",
    "description": "sane validation rule handling for laravel",
    "keywords": ["laravel", "validation"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ohtyap"
        }
    ],
    "require": {
        "illuminate/validation": "^11"
    },
    "require-dev": {
        "illuminate/translation": "^11",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ohtyap\\LaravelSaneValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ohtyap\\LaravelSaneValidator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
